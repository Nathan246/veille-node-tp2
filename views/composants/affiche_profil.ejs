<h2><%=__('profil_de') %> <%= adresses[0].prenom %></h2>
<table class='tableau profil_tab'>
	 <% adresses.forEach(elm => {%>
		 <tr><th><%=__('nom_complet') %></th><td> <%= elm.prenom %> <%= elm.nom %></td></tr>
		 <tr><th><%=__('telephone') %></th><td> <%= elm.telephone %></td></tr>
		 <tr><th><%=__('courriel') %></th><td> <%= elm.courriel %></td></tr>
	 <% }) %>
</table>
<a class="profil_a" href="/adresse">Retour</a>